{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "../frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "../frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "../frontend/build/index.html"
    }
  ]
}
